{
  "name": "hono-compress",
  "version": "0.4.3",
  "type": "module",
  "description": "Compression plugin for Hono working with Bun",
  "author": {
    "name": "Walter Purcaro",
    "url": "https://github.com/vuolter"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/vuolter/hono-compress"
  },
  "main": "./dist/index.cjs",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.cts",
  "exports": {
    "require": {
      "types": "./dist/index.d.cts",
      "default": "./dist/index.cjs"
    },
    "import": {
      "types": "./dist/index.d.mts",
      "default": "./dist/index.mjs"
    }
  },
  "url": "https://github.com/vuolter/hono-compress",
  "keywords": [
    "hono",
    "compression",
    "node",
    "deno",
    "bun",
    "edge",
    "zstd",
    "brotli",
    "gzip",
    "deflate"
  ],
  "license": "MIT",
  "scripts": {
    "build": "pkgroll --clean-dist --minify",
    "test": "bun test",
    "format": "prettier . --write",
    "lint": "eslint --cache --cache-location node_modules/.cache/eslint --cache-strategy content --fix .",
    "typecheck": "tsc --noEmit"
  },
  "peerDependencies": {
    "hono": "^4.6.8"
  },
  "devDependencies": {
    "@eslint/js": "^9.15.0",
    "@types/bun": "^1.1.13",
    "@types/eslint__js": "^8.42.3",
    "@types/node": "^22.9.0",
    "eslint": "9.14.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-import-resolver-typescript": "^3.6.3",
    "eslint-plugin-import-x": "^4.4.2",
    "eslint-plugin-markdown": "^5.1.0",
    "eslint-plugin-perfectionist": "^3.9.1",
    "eslint-plugin-regexp": "^2.7.0",
    "eslint-plugin-security": "^3.0.1",
    "eslint-plugin-sonarjs": "^2.0.4",
    "eslint-plugin-tsdoc": "^0.3.0",
    "eslint-plugin-unicorn": "^56.0.0",
    "pkgroll": "^2.5.1",
    "prettier": "^4.0.0-alpha.10",
    "typescript": "^5.6.3",
    "typescript-eslint": "^8.14.0"
  },
  "dependencies": {
    "@mongodb-js/zstd": "^1.2.2",
    "brotli-wasm": "^3.0.1"
  }
}
